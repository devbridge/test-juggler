{
  "name": "test-juggler",
  "version": "1.1.3",
  "description": "E2E test automation framework by Devbridge",
  "main": "index.js",
  "scripts": {
    "lint": "eslint **/*.js",
    "test": "jest",
    "postinstall": "node ./post-install.js",
    "release": "release-it"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devbridge/test-juggler.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/devbridge/test-juggler",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "async-retry": "^1.0.0",
    "gently-copy": "^3.0.0",
    "jest": "^25.0.0",
    "jest-image-snapshot": "^3.0.0",
    "jest-junit": "^10.0.0",
    "jest-puppeteer": "^4.0.0",
    "puppeteer": "^3.0.0",
    "teremock": "^1.0.5"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "husky": "^4.2.1",
    "release-it": "^13.5.5"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "npx eslint **/*.js",
        "npm run test"
      ]
    },
    "github": {
      "release": true
    }
  },
  "keywords": [
    "testing",
    "automation",
    "e2e",
    "puppeteer",
    "jest"
  ]
}
